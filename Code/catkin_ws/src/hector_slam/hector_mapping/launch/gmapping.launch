<launch>
    <!-- Node for gmapping -->
    <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="screen">
        <!-- Map update rate -->
        <param name="map_update_interval" value="3.0"/>

        <!-- <param name="inverted_laser" value="false"/> -->

        <!-- Parameters related to particle filter -->
        <param name="maxUrange" value="6.0"/>
        <param name="maxRange" value="12.0"/>
        <param name="minimumScore" value="100"/>
        <param name="sigma" value="0.05"/>
        <param name="kernelSize" value="2"/> 
        <param name="lstep" value="0.05"/>
        <param name="astep" value="0.05"/>
        <param name="iterations" value="10"/>
        <param name="lsigma" value="0.075"/>
        <param name="ogain" value="3.0"/>
        <param name="lskip" value="0"/>
        <param name="srr" value="0.1"/>
        <param name="srt" value="0.2"/>
        <param name="str" value="0.1"/>
        <param name="stt" value="0.2"/>
        <param name="linearUpdate" value="1.0"/>
        <param name="angularUpdate" value="0.5"/>
        <param name="resampleThreshold" value="0.2"/>
        <param name="particles" value="100"/>

        <!-- Parameters related to map -->
        <param name="xmin" value="-10.0"/>
        <param name="ymin" value="-10.0"/>
        <param name="xmax" value="10.0"/>
        <param name="ymax" value="10.0"/>
        <param name="delta" value="0.01"/>
        <param name="llsamplerange" value="0.01"/>
        <param name="llsamplestep" value="0.01"/>
        <param name="lasamplerange" value="0.005"/>
        <param name="lasamplestep" value="0.005"/>

        <!-- Parameters for laser sensor -->
        <param name="tf_delay" value="0.05"/>
        <param name="transform_publish_period" value="0.05"/>
        <param name="scan_topic" value="scan"/>

        <!-- Optional parameters -->
        <param name="base_frame" value="base_link"/>
        <param name="odom_frame" value="odom"/>
        <param name="map_frame" value="map"/>
        <param name="throttle_scans" value="1"/>
        <param name="occ_thresh" value="0.25"/>
    </node>

    <!-- Node to provide static transforms -->
    <!-- <node pkg="tf2_ros" type="static_transform_publisher" name="base_to_laser" args="0 0 0 0 0 0 base_link laser"/> -->

  <node pkg="tf" type="static_transform_publisher" name="base_to_laser" args="0 0 0.1 3.1459 0 0 base_link laser 100"/>


    <node pkg="keyboard_ros" type="odom_node.py" name="odom_node"></node>
      <!-- <node pkg="localization_data_pub" type="ekf_odom_pub" name="ekf_odom_pub"> </node>  -->
    

    
</launch>